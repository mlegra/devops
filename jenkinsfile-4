pipeline {
    agent any
    parameters {
        
        string(name: 'Cant', defaultValue: '3', description: 'Cantidad?')
        
       }
    options {
        parallelsAlwaysFailFast()
    }
    stages {
        stage('Non-Parallel Stage') {
            steps {
                echo 'This stage will be executed first.'
            }
        }
        stage('Parallel Stage') {
            
            parallel {
                stage('Branch A') {
                    
                    steps {
                        echo "On Branch A"
                    }
                }
        
            
            }
        }
    }
}